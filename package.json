    "@types/yargs": "^17.0.32",
    "@vitest/coverage-v8": "^3.2.4",
    "prettier": "^3.2.4",
    "tslib": "^2.6.2",
    "typescript": "^5.0.2",
    "vitest": "^3.2.4"
  },
